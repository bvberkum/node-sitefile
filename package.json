{
  "name": "node-sitefile",
  "description": "Bootstrap webserver for misc. resources using Sitefile",
  "version": "0.0.1",
  "homepage": "https://github.com/dotmpe/node-sitefile",
  "author": {
    "name": "B. van Berkum",
    "email": "dev@dotmpe.com"
  },
  "keywords": [
