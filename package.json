  },
  "engines": {
    "node": "0.10.x",
    "npm": "1.2.x"
  },
  "main": "server.coffee",
  "scripts": {
    "start": "NODE_ENV=development nodemon server.coffee",
    "build": "make build-clients",
    "srctree": "tree -I 'node_modules|build|components'",
    "srcloc": "find src/ -iname '*.js' -o -iname '*.coffee' -exec cat {} + | grep -v '^\\s*$' | wc -l"
  },
  "dependencies": {
    "chalk": "^1.0.0",
    "coffee-script": "latest",
    "compression": "^1.0.2",
    "connect-flash": "latest",
    "express": "^4.1.1",
    "jade": "latest",
    "js-yaml": "^3.2.7",
    "lodash": "^3.4.0",
    "method-override": "^2.3.2",
    "node-uuid": "^1.4.3",
    "notifier": "latest"
  },
  "devDependencies": {
  }
